<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts-config PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
    "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
		<form-bean name="page" type="com.szqb.bean.Page" />
		<form-bean name="content" type="com.szqb.bean.Content" />
	</form-beans>

	<action-mappings>
		<action path="/page"
			type="org.apache.struts.beanaction.BeanAction" name="page"
			scope="request" validate="false">
			<forward name="success" path="/jsp/page.jsp" />
		</action>	
		<action path="/content"
			type="org.apache.struts.beanaction.BeanAction" name="content"
			scope="request" validate="false">
			<forward name="success" path="/jsp/content.jsp" />
		</action>	
	</action-mappings>
	<message-resources parameter="MessageResources" />	
</struts-config>
