<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" android:background="@drawable/blue">
	<LinearLayout android:layout_width="fill_parent"
		android:layout_marginBottom="13px" android:layout_marginLeft="13px"
		android:layout_marginRight="13px" android:layout_marginTop="13px"
		android:layout_height="fill_parent" android:orientation="vertical"
		android:background="@drawable/blue_in">
		<ScrollView android:id="@+id/ScrollView01"
			android:layout_width="fill_parent" android:layout_height="fill_parent">
			<LinearLayout android:layout_width="fill_parent"
				android:layout_height="fill_parent" android:orientation="vertical">
				<LinearLayout android:layout_width="fill_parent"
					android:layout_marginLeft="13px" android:layout_marginRight="13px"
					android:layout_marginTop="13px" android:layout_marginBottom="13px"
					android:layout_height="wrap_content">
					<TextView android:text="" android:id="@+id/level"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>

					<TextView android:text="" android:id="@+id/count"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<TextView android:text="" android:id="@+id/price"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:layout_marginLeft="13px"
					android:layout_marginRight="13px">
					<TextView android:text="姓名" android:id="@+id/text1"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:text="" android:id="@+id/name"
						android:layout_width="176px" android:layout_height="wrap_content"
						android:layout_weight="2"></EditText>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:layout_marginLeft="13px"
					android:layout_marginRight="13px">
					<TextView android:text="电话" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:text="" android:id="@+id/tel"
						android:layout_width="176px" android:layout_height="wrap_content"
						android:layout_weight="2"></EditText>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:layout_marginLeft="13px"
					android:layout_marginRight="13px">
					<TextView android:text="手机" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:text="" android:id="@+id/mobil"
						android:layout_width="176px" android:layout_height="wrap_content"
						android:layout_weight="2"></EditText>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:layout_marginLeft="13px"
					android:layout_marginRight="13px">
					<TextView android:text="邮箱" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:text="" android:id="@+id/email"
						android:layout_width="176px" android:layout_height="wrap_content"
						android:layout_weight="2"></EditText>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_marginLeft="13px" android:layout_marginRight="13px"
					android:layout_height="wrap_content">
					<TextView android:text="联系地址" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:text="" android:id="@+id/address"
						android:layout_width="176px" android:layout_height="wrap_content"
						android:layout_weight="2"></EditText>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_marginLeft="13px" android:layout_marginRight="13px"
					android:layout_height="wrap_content">
					<TextView android:text="配送地址" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:text="" android:id="@+id/sentaddress"
						android:layout_width="176px" android:layout_height="wrap_content"
						android:layout_weight="2"></EditText>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_marginLeft="13px" android:layout_marginRight="13px"
					android:layout_height="wrap_content">
					<TextView android:text="旅客数量" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/numSpinner" android:layout_width="176px"
						android:layout_height="wrap_content" android:layout_weight="2"></Spinner>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_marginLeft="13px" android:layout_marginRight="13px"
					android:layout_height="wrap_content">
					<TextView android:text="旅客姓名" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:text="" android:id="@+id/nameoftraveler"
						android:layout_width="176px" android:layout_height="wrap_content"
						android:layout_weight="2"></EditText>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_marginLeft="13px" android:layout_marginRight="13px"
					android:layout_height="wrap_content">
					<TextView android:text="旅客类型" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/travelertype"
						android:layout_width="176px" android:layout_height="wrap_content"
						android:layout_weight="2"></Spinner>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_marginLeft="13px" android:layout_marginRight="13px"
					android:layout_height="wrap_content">
					<TextView android:text="证件类型" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/idtype" android:layout_width="176px"
						android:layout_height="wrap_content" android:layout_weight="2"></Spinner>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_marginLeft="13px" android:layout_marginRight="13px"
					android:layout_height="wrap_content">
					<TextView android:text="证件号码" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<EditText android:text="" android:id="@+id/idnumber"
						android:layout_width="176px" android:layout_height="wrap_content"
						android:layout_weight="2"></EditText>
				</LinearLayout>
				<LinearLayout android:layout_width="fill_parent"
					android:layout_marginLeft="13px" android:layout_marginRight="13px"
					android:layout_height="wrap_content">
					<TextView android:text="保险数量" android:id="@+id/text2"
						android:layout_marginTop="10dip" android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:layout_weight="1"></TextView>
					<Spinner android:id="@+id/insurancenumber"
						android:layout_width="176px" android:layout_height="wrap_content"
						android:layout_weight="2"></Spinner>
				</LinearLayout>
				<RelativeLayout android:layout_width="fill_parent"
					android:layout_marginLeft="13px" android:layout_marginRight="13px"
					android:layout_height="wrap_content">
					<Button android:text="提交" android:id="@+id/commit"
						android:layout_width="100px" android:layout_height="wrap_content"
						android:layout_marginLeft="20px"></Button>
					<Button android:text="返回" android:id="@+id/back"
						android:layout_width="100px" android:layout_height="wrap_content"
						android:layout_alignParentRight="true" android:layout_marginRight="20px"></Button>
				</RelativeLayout>
			</LinearLayout>
		</ScrollView>
	</LinearLayout>

</LinearLayout>
